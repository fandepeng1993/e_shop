.memberCenter {
	width: 100%;	/*width: 63%;
			margin-left: 18.5%;*/
	height: 910px;
	min-width: 1100px;
}

.memberCenter .mem-title {
	width: 100%;
	height: 78px;
	color: #fff;
	min-width: 1100px;
	background: #2C84E7;
}

.memberCenter .mem-title-in {
	width: 63%;
	margin-left: 18.5%;
	min-width: 1100px;
}

.memberCenter .mem-title ul {
	width: 520px;
	float: left;
}

.memberCenter .mem-title ul li {
	display: inline-block;
	font-size: 16px;
	width: 119px;
	height: 73px;
	margin-top: 2px;
	text-align: center;
}

.memberCenter .mem-title ul li a {
	color: #fff;
}

.memberCenter .mem-title ul li p {
	margin-top: -12px;
}

.memberCenter .mem-title ul li.active ,.memberCenter .mem-title ul li.active a{
	background: #fff;
	color: #2C84E7;
}

.memberCenter .mem-title ul li .icon-huiyuanzhongxin {
	font-size: 40px;
}

.memberCenter .mem-title ul li .icon-dianpu {
	font-size: 25px;
	line-height: 50px;
}

.memberCenter .mem-title ul li .icon-shouye1 {
	font-size: 28px;
	line-height: 45px;
}

.memberCenter .mem-title ul li .icon-zhuxiaodenglu {
	font-size: 28px;
	line-height: 50px;
}

.memberCenter .mem-title .mem-title-right {
	display: inline-block;
	font-size: 16px;
}

.memberCenter .mem-title .mem-title-right {
	float: right;
	margin-top: 13px;
	margin-right: 30px;
}

.memberCenter .mem-title .mem-title-right img {
	float: left;
	margin-right: 10px;
}

.memberCenter .mem-title .mem-title-right span.small {
	font-size: 12px;
}

.memberCenter .mem-title .mem-title-right span.small i {
	margin-left: 5px;
	color: #FFE6AA;
}

.memberCenter .mem-content {
	width: 63%;
	margin-left: 18.5%;
	float: left;
	min-width: 1100px;
	margin-top: 0px;
}

.memberCenter .mem-content .mem-left {
	width: 262px;
	height: 477px;
	float: left;
}

.memberCenter .mem-left ul {
	width: 100%;
	height: 100%;
	border: 1px solid #B5B5B5;
	background: #fff;
}

.memberCenter .mem-left ul li {
	width: 100%;
	height: 68px;
	font-size: 18px;
	color: #333;
	padding-top: 22px;
	border-bottom: 1px solid #B5B5B5;
}

.memberCenter .mem-left ul li a {
	color: #333;
}

.memberCenter .mem-left ul li .first {
	padding-left: 25px;
	font-size: 20px;
	margin-right: 10px;
	float: left;
}

.memberCenter .mem-left ul li .icon-jiantou1 {
	float: right;
	margin-right: 20px;
}

.memberCenter .mem-left ul li .icon-shouhuodizhi {
	padding-left: 0px;
	margin-right: -10px;
}

.memberCenter .mem-left ul li .icon-wodexiaoxi {
	font-size: 18px;
	padding-left: 23px;
}

.memberCenter .mem-left ul li .icon-xiugaimima {
	padding-left: 22px;
}

.memberCenter .mem-left ul li .icon-shoucang {
	padding-left: 22px;
}

.memberCenter .mem-left ul li .icon-wuliu1 {
	padding-left: 22px;
}

.memberCenter .mem-left ul li.active ,.memberCenter .mem-left ul li.active a{
	background: #3D91F0;
	color: #fff;
}

.memberCenter .mem-right {
	width: 900px;
	height: 800px;
	margin-left: 10px;
	float: left;
	background: #fff;
	padding: 30px 40px 35px 40px;
	border-top: 4px solid #47A8F9;
}

.memberCenter .mem-right .mem-right-title span.line {
	display: block;
	width: 7px;
	height: 20px;
	background: #2C84E7;
	float: left;
	margin-right: 10px;
	margin-top: 2px;
}

.memberCenter .mem-right .mem-right-title p {
	font-size: 18px;
}

.memberCenter .mem-right .mem-right-bt {
	border: 1px dashed #B5B5B5;
	height: 200px;
}

.memberCenter .mem-right .mem-right-bt  input {
	padding-left: 5px;
	padding-right: 5px;
}

.memberCenter .mem-right .mem-right-bt .mem-right-bt-left {
	width: 38%;
	float: left;
	font-size: 16px;
	color: #333;
	padding-top: 24px;
	padding-left: 20px;
}

.memberCenter .mem-right .mem-right-bt .mem-right-bt-left .mleft {
	width: 100%;
	float: left;
}

.memberCenter .mem-right .mem-right-bt .mem-right-bt-left .mleftT {
	margin-top: 18px;
	width: 100%;
	float: left;
}

.memberCenter .mem-right .mem-right-bt .mem-right-bt-left .check {
	margin-top: 30px;
}

.memberCenter .mem-right .mem-right-bt .mem-right-bt-right {
	width: 62%;
	float: left;
	font-size: 16px;
	color: #333;
	padding-top: 24px;
}

.memberCenter .mem-right .mem-right-bt .mem-right-bt-right ul {
	width: 100%;
	float: right;
}

.memberCenter .mem-right .mem-right-bt .mem-right-bt-right ul li {
	display: inline-block;
	width: 31%;
	margin-top: 20px;
	text-align: right;
}

.memberCenter .mem-right .mem-right-bt .mem-right-bt-right .btn {
	float: right;
	background: #2C84E7;
	color: #fff;
	margin-top: 40px;
	margin-right: 28px;
}

.memberCenter .mem-right .mem-right-ul {
	width: 100%;
}

.memberCenter .mem-right .mem-right-ul ul {
	width: 100%;
	height: 38px;
	background: #B5B5B5;
	margin-top: 21px;
	border: 1px solid #ccc;
}

.memberCenter .mem-right .mem-right-ul ul li {
	display: inline-block;
	width: 16%;
	text-align: center;
	line-height: 34px;
	font-size: 16px;
	border-right: 1px solid #959595;
	position: relative;
}

.memberCenter .mem-right .mem-right-ul ul li .sanjiao {
	position: absolute;
	width: 0;
	height: 0;
	border-left: 4px solid transparent;
	border-right: 4px solid transparent;
	border-bottom: 5px solid #FF6600;
	left: 50%;
	margin-left: -2px;
	bottom: 0px;
	display: none;
}

.memberCenter .mem-right .mem-right-ul ul li.active {
	background: #fff;
	border-bottom: 2px solid #FF6600;
}

.memberCenter .mem-right .mem-right-ul ul li.active .sanjiao {
	display: block;
}

.memberCenter .mem-right .mem-right-ul ul li.last {
	border-right: 0px;
}

.memberCenter .mem-right .mem-table {
	margin-top: 20px;
	font-size: 14px;
}

.memberCenter .mem-right .mem-table td {
	vertical-align: middle;
}

.memberCenter .mem-right .mem-table td.orange {
	color: #FF6600;
}

.memberCenter .mem-right .mem-table td .btn {
	background: #FF6600;
	color: #fff;
	outline: none;
	border-radius: 20px;
	padding-left: 15px;
	padding-right: 15px;
}

.memberCenter .mem-right .mem-table td p.text-close {
	color: #999;
}

.memberCenter .mem-right .mem-table td span.line {
	width: 4px;
	height: 15px;
	background: #2C84E7;
	display: block;
	float: left;
	margin-left: -10px;
	margin-top: 3px;
}

.memberCenter .mem-right .mem-table td span.order {
	padding-right: 10px;
}

.memberCenter .mem-right .mem-table td span.mem-r {
	float: right;
	padding-right: 10px;
	color: #2C84E7;
}

/*小屏适配*/
.smallScreen .memberCenter .mem-title-in {
	width: 80%;
	margin-left: 10%;
}

.smallScreen .memberCenter .mem-content {
	width: 80%;
	margin-left: 10%;
	float: left;
}

.smallScreen .memberCenter .mem-right {
	width: 815px;
}

.smallScreen .memberCenter .mem-right .mem-right-bt  input[type="text"] {
	width: 165px;
}
