.custom-classify{
    position: absolute;
    bottom: 4px;
    width: 13%;
    min-width: 10%;
    left: 20%;
    height: 332px;
    background: #fff;
    box-shadow: 0 0px 0 4px #3d9ae9;
    -webkit-box-shadow: 0 0px 0 4px #3d9ae9;
    -ms-box-shadow: 0 0px 0 4px #3d9ae9;
    -moz-box-shadow: 0 0px 0 4px #3d9ae9;
    -O-box-shadow: 0 0px 0 4px #3d9ae9;
}
.custom-classify-top{
    background:#3d9ae9;
    color: white;
    overflow: hidden;

}
.classTop{
    float: left;
    font-size: 30px;
    margin-left: 10%;

}
.classTops{
    float: left;
    font-size: 15px;
    padding: 6%;
    font-family: 仿宋体;
}
.custom-classify-top-ul>li .icon-zuanshi{
    font-size: 40px;
    float: left;
}
.custom-classify-top-ul>li{
    overflow: hidden;
    cursor: pointer;
    padding:0 4%;
}
.custom-classify-top-ul>li span{
    float: left;
    margin-top: 20px;
    font-size:15px;
    font-weight: bold;
    font-family: 仿宋体;
}
.custom-classify-top-ul>li b{
    float: right;
    margin-top: 20px;
    margin-right: 5px;
    font-weight: normal;
    color: #9a9a9a;
    font-family: 宋体;
}
i.statered{
    color:#ffa7a7 ;
}
i.stateyellow{
    color:#fdd089 ;
}
i.stateblue{
    color: #91bdff;
}
i.statesky{
    color: #8ff0dd;
}
i.statepink{
    color:#f5b2ff ;
}
.customsearch{
    position: absolute;
    width: 100%;
    bottom: 20%;
}
.customsearch .customsearch-center{
    margin: 0 auto;
    width: 30%;
    overflow: hidden;
}
.customsearch input{
    float: left;
    width: 84%;
}
.customsearch button{
    float: right;
}
.FindCustomMade-more-ul{
    overflow: hidden;
}

.FindCustomMade-more-ul li{
    float: left;
    width: 23%;
    background: #fff;
    margin: 1%;
    cursor: pointer;
}
.FindCustomMade-more-ul li h4{
    font-size: 15px;
    height: 60px;
    line-height: 20px;
    margin-top: 0;
}
.FindCustomMade-more-ul li h4 span{
    display: inline-block;
    margin-right: 10px;
}
.FindCustomMade-more-ul li img{
    width: 100%;
}
.FindCustomMade-more-ul li p {
    font-size: 10px;
    color: #999999;
    overflow: hidden;
    margin:0;
}
.company{float: left}
.laddress{float: right}
.FindCustomMade-more-ul-wrap{
    padding: 10px 10px;
}
ol.breadcrumb{
    margin-bottom: 10px;
}