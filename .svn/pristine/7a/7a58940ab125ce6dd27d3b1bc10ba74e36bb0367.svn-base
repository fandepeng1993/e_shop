.title-shoppingCart {
	float: left;
	width: 123px;
	height: 47px;
	line-height: 47px;
	text-align: center;
	margin-top: 26px;
	margin-left: 2.7%;
	border: 1px solid #3D91F0;
	background-color: #fff;
	outline: none;
}
.title-shoppingCart i {
	font-size: 22px;
	color: #3D91F0;
}
.title-shoppingCart .shoppingCart {
	display: inline-block;
	margin-left: 8px;
	font-size: 14px;
	color: #666666;
}
.title-shoppingCart .shoppingNum {
	display: inline-block;
	margin-left: 6px;
	width: 21px;
	height: 20px;
	line-height: 20px;
	background-color: #3D91F0;
	border-radius: 50%;
	position: relative;
	z-index: 1;
	behavior: url(../../common/css/ie-css3.htc);
}

.SellerCenter {
	width: 100%;
	min-width: 1100px;
	margin-bottom: 114px;
}

.SellerCenter .mem-title {
	width: 100%;
	height: 78px;
	color: #fff;
	min-width: 1100px;
	background: #2C84E7;
}

.SellerCenter .mem-title-in {
	min-width: 1100px;
	width: 62.5%;
	margin: 0 auto;
	overflow: hidden;
}

.SellerCenter .mem-title ul {
	float: left;
}

.SellerCenter .mem-title ul li {
	display: inline-block;
	font-size: 16px;
	width: 119px;
	height: 73px;
	margin-top: 2px;
	text-align: center;
}

.SellerCenter .mem-title ul a{
	color: #fff;
}

.SellerCenter .mem-title ul li p {
	margin-top: -12px;
}

.SellerCenter .mem-title ul li.active {
	background: #fff;
	color: #2C84E7;
}

.SellerCenter .mem-title ul li.active a{
	background: #fff;
	color: #2C84E7;
}

.SellerCenter .mem-title ul li .icon-huiyuanzhongxin {
	font-size: 40px;
}

.SellerCenter .mem-title ul li .icon-dianpu {
	font-size: 25px;
	line-height: 57px;
}

.SellerCenter .mem-title ul li .icon-shouye1 {
	font-size: 28px;
	line-height: 45px;
}

.SellerCenter .mem-title ul li .icon-zhuxiaodenglu {
	font-size: 28px;
	line-height: 50px;
}

.SellerCenter .mem-title .mem-title-right {
	display: inline-block;
	font-size: 16px;
}

.SellerCenter .mem-title .mem-title-right {
	float: right;
	margin-top: 13px;
	margin-right: 30px;
}

.SellerCenter .mem-title .mem-title-right img {
	float: left;
	margin-right: 10px;
}

.SellerCenter .mem-title .mem-title-right span.small {
	font-size: 12px;
}

.SellerCenter .mem-title .mem-title-right span.small i {
	margin-left: 5px;
	color: #FFE6AA;
}

.SellerCenter .mem-content {
	width: 62.5%;
	min-width: 1100px;
	margin: 17px auto 0;
	overflow: hidden;
}

.SellerCenter .mem-content .mem-left {
	width: 21.84%;
	height: 477px;
	float: left;
}

.SellerCenter .mem-left ul {
	width: 100%;
	height: 100%;
	border: 1px solid #B5B5B5;
	background: #fff;
}

.SellerCenter .mem-left ul li {
	width: 100%;
	height: 68px;
	font-size: 18px;
	color: #333;
	padding-top: 22px;
	border-bottom: 1px solid #B5B5B5;
}

.SellerCenter .mem-left ul li a {
	color: #333;
}

.SellerCenter .mem-left ul li .first {
	padding-left: 25px;
	font-size: 20px;
	margin-right: 10px;
	float: left;
}

.SellerCenter .mem-left ul li .icon-jiantou1 {
	float: right;
	margin-right: 20px;
}

.SellerCenter .mem-left ul li .icon-shouhuodizhi {
	padding-left: 0px;
	margin-right: -10px;
}

.SellerCenter .mem-left ul li .icon-wodexiaoxi {
	font-size: 18px;
	padding-left: 23px;
}

.SellerCenter .mem-left ul li .icon-xiugaimima {
	padding-left: 22px;
}

.SellerCenter .mem-left ul li .icon-shoucang {
	padding-left: 22px;
}

.SellerCenter .mem-left ul li .icon-wuliu1 {
	padding-left: 22px;
}

.SellerCenter .mem-left ul li.active ,.SellerCenter .mem-left ul li.active a{
	background: #3D91F0;
	color: #fff;
}

.SellerCenter .mem-right {
	width: 77.33%;
	margin-left: 0.83%;
	float: left;
	background: #fff;
	border-top: 4px solid #47A8F9;
	border-left: 1px solid #E5E5E5;
	border-right: 1px solid #E5E5E5;
	border-bottom: 1px solid #E5E5E5;
}

.mem-right-title {
	overflow: hidden;
	height: 53px;
	line-height: 53px;
	background: #eee;
	border-bottom: 1px solid #BCBCBC;
}
.SellerCenter .mem-right .mem-right-ul{
	float: left;
	height: 53px;
}

.SellerCenter .mem-right .mem-right-ul li{
	width: 152px;
	height: 53px;
	line-height: 53px;
	font-size: 18px;
	color: #333;
	text-align: center;
	float: left;
	border-right: 1px solid #BCBCBC;
	position: relative;
	cursor: pointer;
}

.SellerCenter .mem-right .mem-right-ul li .sanjiao{
	position: absolute;
	width: 0;
	height: 0;
	border-left: 4px solid transparent;
	border-right: 4px solid transparent;
	border-bottom: 5px solid #FF6600;
	left: 50%;
	margin-left: -2px;
	bottom: 0px;
	display: none;
}

.SellerCenter .mem-right .mem-right-ul li.active{
	background: #fff;
	border-bottom: 3px solid #FF6600;
}

.SellerCenter .mem-right .mem-right-ul li.active .sanjiao{
	display: block;
}
.mem-right-ul-l>li.ACtive {
	position: relative;
	display: block;
}

.mem-right-ul-l>li {
	display: none;
}

.ACtive {
	padding: 44px 0 0 3.5%;
}
.form-top {
	overflow: hidden;
}
.form-top3 {
	margin-top: 30px;
}
.form-top-left,.form-top-right {
	float: left;
}
.form-top-left {
	width: 19%;
}
.form-top .circular {
	display: inline-block;
	width: 5px;
	height: 5px;
	background-color: #E60012;
	border-radius: 50%;
	vertical-align: top;
	margin-top: 25px;
	position: relative;
    z-index: 1;
    behavior: url(../../common/css/ie-css3.htc);
}
.form-top label {
	font-size: 16px;
	font-weight: normal;
	margin: 15px 0 0 9px;
	vertical-align: top;
}
.form-top .icon-tianjiatupian {
	font-size: 89px;
	color: #ccc;
	margin-right: 31px;
}
.form-top .datu {
	position: relative;
	top: -30px;
}
.form-top .ico-close {
	position: relative;
	top: -73px;
	left: -14px;
}
.form-top .notice {
	display: inline-block;
	width: 336px;
	height: 42px;
	line-height: 42px;
	font-size: 12px;
	text-align: center;
	border: 1px dotted #BFBFBF;
	position: relative;
	top: -25px;
	margin-left: 30px;
}
.form-top-right .img2,.form-top-right .img4 {
	margin-left: 10px;
}
.form-top-right .img3,.form-top-right .img4 {
	margin-top: 10px;
}
.form-top-right1 {
	margin-top: 15px;
	width: 81%;
}
.form-top-right textarea {
	width: 647px;
	height: 189px;
	padding: 10px;
}
.save-btn {
	text-align: center;
}
.save-btn button {
	width: 145px;
	height: 36px;
	background-color: #2C84E7;
	border: 1px solid #2C84E7;
	border-radius: 4px;
	color: #fff;
	font-size: 16px;
	margin: 26px 0;
	position: relative;
    z-index: 1;
    behavior: url(../../common/css/ie-css3.htc);
}
