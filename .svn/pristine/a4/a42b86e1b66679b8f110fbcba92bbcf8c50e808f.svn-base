}

.home-auto-scroll:hover {
	cursor: pointer;
}

.home-auto-scroll {
	height: 180px;
	overflow-y: hidden;
}

.home-Info-title {
	text-align: center;
}

.home-Info-title a {
	margin: 20px;
	padding: 10px;
	color: #333333;
	text-decoration: none;
}

.home-Info-title a:hover {
