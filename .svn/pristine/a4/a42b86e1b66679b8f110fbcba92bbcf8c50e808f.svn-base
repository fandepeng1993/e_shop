}

.home-Info-content {
	margin-top: 40px;
}

.home-Info-content ul {
	display: block;	/*list-style-type: disc;*/
	color: #F39800;
	padding-right: 20px;	/*padding-left: 40px;*/
}

.home-Info-content ul li i {
	font-style: normal;
	float: left;
	width: 10%;
	text-align: center;
}

.home-Info-content li a {
	color: #333;
	float: left;
	width: 60%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.home-Info-content li a:hover {
	color: #333;
	text-decoration: none;
	cursor: pointer;
}

.home-Info-content ul {
	display: none;
}

.home-Info-content ul li {
	margin: 30px 0px 30px 0px;
}

.home-Info-content ul li .home-Info-time {
	color: #999999;
	font-size: 12px;
	float: right;
	width: 30%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.home-Info-content ul.AcTive {
	display: block;
}

.home-Market-content ul {
	display: block;
	color: #F39800;
	padding-right: 20px;
}

.home-Market-content ul li i {
	font-style: normal;
	float: left;
	width: 10%;
	text-align: center;
}

.home-Market-content ul li {
	margin: 0px 0px 28px 0px;
}

.home-Market-content ul li .home-Info-time {
	color: #999999;
	font-size: 12px;
	float: right;
	width: 30%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.home-Market-content li a {
	color: #333;
	float: left;
	width: 60%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	cursor: pointer;
}

.home-Market-content li a:hover {
