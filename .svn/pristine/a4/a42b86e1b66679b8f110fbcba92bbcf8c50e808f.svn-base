/*home.css*/
.home-former {
	position: absolute;
	bottom: 4px;
	width: 40%;
	min-width: 546px;
	left: 20%;
	height: 332px;
	background: #fff;
	box-shadow: 0 0px 0 4px rgba(0, 0, 0, 0.2);
	-webkit-box-shadow: 0 0px 0 4px rgba(0, 0, 0, 0.2);
	-ms-box-shadow: 0 0px 0 4px rgba(0, 0, 0, 0.2);
	-moz-box-shadow: 0 0px 0 4px rgba(0, 0, 0, 0.2);
	-O-box-shadow: 0 0px 0 4px rgba(0, 0, 0, 0.2);
}

body{
	background: #f5f5f5!important;
}

/*.flex-direction-nav a{width:60px;height:90px;line-height:99em;overflow:hidden;margin:-60px 0 0;display:block;background:yellow;position:absolute;top:50%;z-index:10;cursor:pointer;opacity:0;filter:alpha(opacity=0);-webkit-transition:all .3s ease;}*/
.flex-direction-nav a {
	display: none
}

.flex-direction-nav .flex-next {
	background-position: 0 -90px;
	right: 0;
}

.flex-direction-nav .flex-prev {
	left: 0;
}

.flexslider:hover .flex-next {
	opacity: 0.8;
	filter: alpha(opacity=25);
}

.flexslider:hover .flex-prev {
	opacity: 0.8;
	filter: alpha(opacity=25);
}

.flexslider:hover .flex-next:hover, .flexslider:hover .flex-prev:hover {
	opacity: 1;
	filter: alpha(opacity=50);
}

.flex-control-nav {
	width: 100%;
	position: absolute;
	bottom: 10px;
	text-align: center;
	list-style: none;
	display: inline-block;
	width: 10px;
	padding: 0px;
	right: 16%;
	top: 50%;
	transform: translateY(-50%);
	-webkit-transform: translateY(-50%);
	-moz-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	-O-transform: translateY(-50%);
}

.flex-control-nav li {
	margin: 5px 0;
	zoom: 1;
	*display: inline;
}

.flex-control-paging li a {
	background: url(../../material/home/dot.png) no-repeat 0 -16px;
	display: block;
	height: 16px;
	overflow: hidden;
	text-indent: -99em;
	width: 16px;
	cursor: pointer;
}

.flex-control-paging li a.flex-active {
	background-position: 0 0;
}

.home-f-left {
	width: 25%;
	float: left;
	margin: 0;
	text-align: center;
	height: 332px;
}

.home-f-ul {
	list-style: none;
	padding: 0;
}

.home-f-ul li {
	color: #fff;
	height: 50px;
	padding: 16.5px 0;
	box-sizing: content-box;
	-webkit-box-sizing: content-box;
	-moz-box-sizing: content-box;
	-ms-box-sizing: content-box;
	-O-box-sizing: content-box;
	background: #3467EB;
	cursor: pointer;
	font-size: 16px;
	line-height: 50px;
	text-align: center;
}

.home-f-ul li.one i{
	font-size: 35px;
	float: left;
	margin-left: 45px;
}

.home-f-ul li.two i{
	font-size: 25px;
	float: left;
	margin-left: 45px;
}

.home-f-ul li.three i{
	font-size: 30px;
	float: left;
	margin-left: 40px;
}

.home-f-ul li.four i{
	font-size: 30px;
	float: left;
	margin-left: 35px;
}

.home-f-ul li.three span{
	margin-left: -40px;
}

.home-f-ul li a{
	color: #fff;
	text-decoration: none;
}

.home-f-ul li span {
	margin-left: -38px;
}

.home-f-ul li.active {
	background: #fff;
	color: #333333;
}

.home-fshow-ul {
	list-style: none;
	padding: 0;
}

.home-fshow-ul>li {
	display: none;
}

.home-fshow-ul>li.active {
	display: block;
}

.home-fsu-ul {
	text-align: center;
	padding: 0;
	list-style: none;
}

.home-fsu-ul>li {
	float: left;
	width: 27.3%;
	margin: 2% 3% 0 3%;
	position: relative;
	height: 30px;
}

.home-fsu-ul>li:hover {
	cursor: pointer;
}

.home-fsu-ul>li.acTive {
	border-bottom: 2px solid #3467EB;
	position: relative;
}

.home-fsu-ul li.acTive i {
	position: absolute;
	width: 0;
	height: 0;
	border-left: 3px solid transparent;
	border-right: 3px solid transparent;
	border-bottom: 4px solid #3467EB;
	bottom: 0px;
	left: 50%;
}

.home-f-right {
	float: left;
	width: 75%;
	height: 332px;
}

.home-fsus-ul>li {
	display: none;
}

.home-fsus-ul>li.ACtive {
	display: block;
}

.home-fsus-ul .c_left {
	float: left;
	width: 48%;
}

.home-fsus-ul .c_right {
	float: right;
	width: 48%;
}

.home-fsus-ul .btDs {
	margin-bottom: 20px;
}

.home-fsus-ul .btn-info {
	float: right;
	margin-top: 20px;
}

.home-fsuls-list {
	font-size: 12px;
	margin: 30px 20px 0 20px;
	height: 25px;
	overflow: hidden;
}

.home-fsuls-list a {
	float: left;
}

.home-fsuls-list a:hover {
	cursor: pointer;
	text-decoration: none;
}

.home-fsuls-list-ul li {
	height: 25px;
	line-height: 25px;
}

.home-fsus-ul-find {
	padding: 20px;
	background: #eee;
	margin: 20px;
	height: 200px;
}

.home-fsuls-list p {
	float: right;
	margin: 0;
}

.home-uploadc {
	position: absolute;
