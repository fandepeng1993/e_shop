.home-cooperation{
	min-width: 1200px;
	width: 63%;
	margin-left: 19%;
}
.home-cooperation >.home-cooperation-title {
	padding-left: 20px;
	margin: 30px 0;
}

.home-cooperation-title {
	padding-left: 20px;
}

.home-cooperation-title i.iconfont {
	font-size: 46px;
	float: left;
	color: #3d9ae9;
}

.home-cooperation-title .clearfixs a {
	color: #7D7D7D;
	float: right;
	margin-right: 20px;
	margin-top: 10px;
}

.home-cooperation-title a:hover {
	text-decoration: none;
	cursor: pointer;
}

.home-cooperation-title h5, .home-cooperation-title p {
	margin: 0;
}

.home-cooperation-title h5 {
	margin: 5px 0;
}

.home-cooperation-title-text {
	float: left;
	margin-left: 10px;
	margin-top: 6px;
}

.home-cooperation-ul {
	margin: 0;
}

.home-cooperation-ul li.first {
	box-shadow: 0px 0px 8px #ccc;
}

.home-cooperation-ul li p {
	margin: 36px 0;
}

.home-cooperation-ul>li {
	float: left;
	width: 16%;
	min-width: 182px;
	padding-top: 20px;
	margin: 0 2%;
	background: #fff;
	text-align: center;
	position: relative;
}

.home-cooperation-ul li img {
