	background: #fff;
	height: 280px;
	min-width: 299px;
	padding-top: 12px;
}

.home-hot-recommend-ul .hot-list-recommend li {
	width: auto;
	margin-bottom: 12px;
	float: none;
	display: block;
}

.hot-list-recommend p {
	font-size: 12px;
	padding: 4px 0 4px 0;
}

.p-color {
	color: #999;
}

.p-color span {
