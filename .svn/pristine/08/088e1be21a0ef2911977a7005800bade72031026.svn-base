.title-shoppingCart {
	float: left;
	width: 123px;
	height: 47px;
	line-height: 47px;
	text-align: center;
	margin-top: 26px;
	margin-left: 2.7%;
	border: 1px solid #3D91F0;
	background-color: #fff;
	outline: none;
}
.title-shoppingCart i {
	font-size: 22px;
	color: #3D91F0;
}
.title-shoppingCart .shoppingCart {
	display: inline-block;
	margin-left: 8px;
	font-size: 14px;
	color: #666666;
}
.title-shoppingCart .shoppingNum {
	display: inline-block;
	margin-left: 6px;
	width: 21px;
	height: 20px;
	line-height: 20px;
	background-color: #3D91F0;
	border-radius: 50%;
	position: relative;
	z-index: 1;
	behavior: url(../../common/css/ie-css3.htc);
}

.receivingAd {
	width: 100%;
	height: 830px;
	min-width: 1100px;
}

.receivingAd .mem-title {
	width: 100%;
	height: 78px;
	color: #fff;
	min-width: 1100px;
	background: #2C84E7;
}

.receivingAd .mem-title-in {
	min-width: 1100px;
	width: 62.5%;
	margin: 0 auto;
	overflow: hidden;
}

.receivingAd .mem-title ul {
	float: left;
}

.receivingAd .mem-title ul li {
	display: inline-block;
	font-size: 16px;
	width: 119px;
	height: 73px;
	margin-top: 2px;
	text-align: center;
}

.receivingAd .mem-title ul a{
	color: #fff;
}

.receivingAd .mem-title ul li p {
	margin-top: -12px;
}

.receivingAd .mem-title ul li.active {
	background: #fff;
	color: #2C84E7;
}

.receivingAd .mem-title ul li.active a{
	background: #fff;
	color: #2C84E7;
}

.receivingAd .mem-title ul li .icon-huiyuanzhongxin {
	font-size: 40px;
}

.receivingAd .mem-title ul li .icon-dianpu {
	font-size: 25px;
	line-height: 50px;
}

.receivingAd .mem-title ul li .icon-shouye1 {
	font-size: 28px;
	line-height: 45px;
}

.receivingAd .mem-title ul li .icon-zhuxiaodenglu {
	font-size: 28px;
	line-height: 50px;
}

.receivingAd .mem-title .mem-title-right {
	display: inline-block;
	font-size: 16px;
}

.receivingAd .mem-title .mem-title-right {
	float: right;
	margin-top: 13px;
	margin-right: 30px;
}

.receivingAd .mem-title .mem-title-right img {
	float: left;
	margin-right: 10px;
}

.receivingAd .mem-title .mem-title-right span.small {
	font-size: 12px;
}

.receivingAd .mem-title .mem-title-right span.small i {
	margin-left: 5px;
	color: #FFE6AA;
}

.receivingAd .mem-content {
	width: 62.5%;
	min-width: 1100px;
	margin: 17px auto 0;
	overflow: hidden;
}

.receivingAd .mem-content .mem-left {
	
	width: 21.84%;
	height: 477px;
	float: left;
}

.receivingAd .mem-left ul {
	width: 100%;
	height: 100%;
	border: 1px solid #B5B5B5;
	background: #fff;
}

.receivingAd .mem-left ul li {
	width: 100%;
	height: 68px;
	font-size: 18px;
	color: #333;
	padding-top: 22px;
	border-bottom: 1px solid #B5B5B5;
}

.receivingAd .mem-left ul li a {
	color: #333;
}

.receivingAd .mem-left ul li .first {
	padding-left: 25px;
	font-size: 20px;
	margin-right: 10px;
	float: left;
}

.receivingAd .mem-left ul li .icon-jiantou1 {
	float: right;
	margin-right: 20px;
}

.receivingAd .mem-left ul li .icon-shouhuodizhi {
	padding-left: 0px;
	margin-right: -10px;
}

.receivingAd .mem-left ul li .icon-wodexiaoxi {
	font-size: 18px;
	padding-left: 23px;
}

.receivingAd .mem-left ul li .icon-xiugaimima {
	padding-left: 22px;
}

.receivingAd .mem-left ul li .icon-shoucang {
	padding-left: 22px;
}

.receivingAd .mem-left ul li .icon-wuliu1 {
	padding-left: 22px;
}

.receivingAd .mem-left ul li.active ,.receivingAd .mem-left ul li.active a{
	background: #3D91F0;
	color: #fff;
}

.receivingAd .mem-right {
	width: 77.33%;
	height: 476px;
	margin-left: 0.83%;
	float: left;
	background: #fff;
	padding: 30px 40px 35px 40px;
	border-top: 4px solid #47A8F9;
	border-left: 1px solid #E5E5E5;
	border-right: 1px solid #E5E5E5;
	border-bottom: 1px solid #E5E5E5;
}

.receivingAd .mem-right .mem-right-title span.line {
	display: block;
	width: 7px;
	height: 20px;
	background: #2C84E7;
	float: left;
	margin-right: 10px;
	margin-top: 2px;
}
.receivingAd .mem-right .mem-right-title {
	overflow: hidden;
	border-bottom: 1px solid #BFBFBF;
	padding-bottom: 13px;
}
.receivingAd .mem-right .mem-right-title p {
	font-size: 18px;
	float: left;
	margin: 0;
}
.receivingAd .mem-right .mem-right-title button {
	float: right;
	width: 117px;
	height: 32px;
	color: #2E2E2E;
	border: 1px solid #1779CE;
	background-color: #fff;
}
.add-new-addr i {
	color: #999;
}
.men-right-table table {
	width: 100%;
}
.men-right-table table>thead>tr,.men-right-table table>tbody>tr {
	border: 1px solid #BFBFBF;
}
.men-right-table table>thead>tr>th {
	font-size: 16px;
	height: 46px;
	background-color: #EAEAEA;
}
.men-right-table table>thead>tr>th,.men-right-table table>tbody>tr>td {
	text-align: center;
}
.men-right-table table>tbody>tr>td {
	height: 61px;
	background-color: #fff;
}
.men-right-table table>tbody>tr>td .icon-shanchu {
	font-size: 22px;
	color: #C8C8C8;
}
.first-shanchu .icon-shanchu {
	color: #5C97D8 !important;
}
.receive-name {
	width: 18%;
}
.operation {
	width: 15%;
}



