#myTabs .nav-tabs{
    background: #FFFDEF;
    border-left: 1px solid #dcdcdc;
    border-right: 1px solid #dcdcdc;
    border-bottom: 1px solid #dcdcdc;
}

#myTabs .nav-tabs>li.active>a,#myTabs .nav-tabs>li.active>a:focus,#myTabs .nav-tabs>li.active>a:hover{
    border-radius: 0;
    border-left:none;
}
#myTabs .nav-tabs>li.active>a:after{
    content:'';
}

#myTabs .nav-tabs>li>a{
    margin-right: 0px;
    border-radius: 0px;
    width: 122px;
    text-align: center;
    padding: 13px 15px;
    border-right: 1px solid #dcdcdc;
}

.page_div {
    width: 75%;
    margin: 28px auto 0px;
    font-size: 14px;
    color: #333;
    padding-bottom: 32px;
}
.totalPages {
    margin: 0 10px;
}
.totalPages span, .totalSize span {
    color: #0073A9;
    margin: 0 5px;
}
#prePage, #nextPage {
    width: 66px;
}
.page_div a {
    min-width: 31px;
    height: 28px;
    border: 1px solid #dce0e0!important;
    text-align: center;
    margin-left: 4px;
    cursor: pointer;
    line-height: 28px;
    color: #666666;
    font-size: 13px;
    display: inline-block;
}
.page_div .current {
    background-color: #0073A9;
    border-color: #0073A9;
    color: #FFFFFF;
    margin-left: 9px;
}

.tab-content{
    margin-top: 20px;
}
.tab-content-ul>li{
    overflow: hidden;
    border: 1px solid #dcdcdc;
    padding: 10px;
}