    margin-top: 1.3%;
    background: #fff;
}
.fab-example-wrap{
    padding:30px 5% 0 5%;
    background: #E7F5FF;
    border-bottom: 1px solid #D2D2D2;
    position: relative;
}
.fab-example-wrap h4{
    font-size: 15px;
    font-weight:bold;
    color: #2C7AE4;
    margin:0 0 10px 0;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;

}
.fab-example-wrap p{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: break-word;
    word-break: break-all;
    font-size: 13px;
}
.fab-example-wrap p span{
    color: #FF4D4D;
}
.fab-state-wrap{
    position: absolute;
    top:0px;
    width:95%;
}
.fab-state{
    font-size: 10px;
    display:block;
    text-align: center;
    width: 30%;
    font-style: normal;
    margin: 0 auto;
    color: white;
}
.fab-state.fab-success{
    background: #73ADFB;
}
.fab-state.fab-warn{
    background:#FFC323;
}
.fab-state.fab-danger{
    background:#F19EC2
}
.fab-a-example{
    padding:5% ;
    border-bottom: 1px solid #DCDCDC;
}
.fab-a-example li{
    margin-bottom: 5%;
    overflow: hidden;
}
.fab-a-example li i{
    font-style: normal;
    display: inline-block;
    width: 39%;
    float: left;
}
.fab-a-example li span{
    display: inline-block;
    font-style: normal;
    width: 58%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    float: left;
}
.fab-qiangdan{
    width: 42%;
    margin: 0 auto;
    text-align: center;
    margin-bottom: 20px;
    margin-top: 25px;
}
.fab-qiangdan a{
    padding: 5px 15px;
    background: #FFB924;
    color: white;
    cursor: pointer;
    border-radius: 5px;
}
.fab-qiangdan a:hover{
    text-decoration: none;
    background: #F38400;

}
.FindAskbuy-content-right-wrap>li{
    background: #fff;
    border:1px solid #D2D2D2;
    margin-bottom: 30px;
}
.top-title h4{
    margin: 0;
    padding: 5%;
    font-size: 15px;
    font-weight: bold;
    border-bottom: 1px solid #D2D2D2;
}
.FindAskbuy-content-right-wrap .top-title{
    background:#E8E8E8;
    text-align: center;
}
.FindAskbuy-content-right-wrap .top-title a{
