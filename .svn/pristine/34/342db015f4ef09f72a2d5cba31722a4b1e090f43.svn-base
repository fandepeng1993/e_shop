}

.FindAskbuy{
    margin-top: 3%;
}
.FindAskbuy .FindAskbuy-content{
    margin-top: 80px;
}
.FindAskbuy-content-left{
    float: left;
    width: 78%;
    margin-right: 2%;
}
.FindAskbuy-content-right{
    float: left;
    width: 20%;
    /*height: 400px;
    background:cyan;*/
}
.FindAskbuy-content-left-top{
    padding-left:20px;
    background: #fff;
}
.FindAskbuy-content-left-top i.iconfont{
    font-size: 46px;
    float: left;
    color: #3d9ae9;
}
.buy-info-title-text{
    float: left;
    margin-left: 10px;
    margin-top: 6px;
}
.buy-info-title-text h4{
    margin:5px 0;
}

.FindAskbuy-content-left-top a{
    color: #333;
    float: right;
    margin-right: 20px;
    margin-top: 20px;
    cursor: pointer;
}
.FindAskbuy-content-left-top a span
{
    color: #FF9B0C;
}
.distance{
    margin:0 1.3%;
}
.rdistance{
    margin-left: 0;
}
.FindAskbuy-content-left-down-list>ul>li{
    width: 24%;
