    left: 50%;
    margin-top: -301px;
    margin-left: -29.8%;
    padding: 0 36px;
    background-color: #fff;
    z-index: 801;
    opacity: 1;
    /*filter: progid:DXImageTransform.Microsoft.Alpha(opacity=70);兼容IE8透明度*/
