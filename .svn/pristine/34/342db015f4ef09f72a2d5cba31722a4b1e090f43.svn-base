    cursor: pointer;
    font-weight: normal;
    color: #666666;
}
.top-title a:hover{
    text-decoration: none;
}
.FindAskbuy-content-right-top-ul p{
    overflow: hidden;
}
.FindAskbuy-content-right-top-ul li{
    margin:20px 4% 4% 4%;
    border-bottom: 1px dashed #BFBFBF;
    padding-bottom: 20px;
}
.FindAskbuy-content-right-top-ul li.last-li{
    border-bottom: none;
}
.FindAskbuy-content-right-top-ul p a{
    float: left;
    width: 75%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    cursor: pointer;

}
.FindAskbuy-content-right-top-ul p time{
    float: right;
    width: 25%;
    text-align: right;
    font-size: 12px;
    color: #666;
}
.FindAskbuy-content-right-top-ul .fab-label span{
    font-size: 12px;
    color: #666;
    display: inline-block;
    margin: 2px;
}
.FindAskbuy-content-right-down-ul{
    padding-top: 10px;
}

.FindAskbuy-content-right-down-ul li{
    padding:4%;
    overflow: hidden;
    border-bottom: 1px solid #BFBFBF;
}
.FindAskbuy-content-right-down-ul li i{
    font-style: normal;
    float: left;
    width: 5%;
    text-align: left;
    color: #D2D2D2;
}
.FindAskbuy-content-right-down-ul li a{
    float: left;
    width: 95%;
    text-decoration: none;
    cursor: pointer;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.page_div{
    width: 75%;
    margin: 24px auto 32px;
    font-size: 14px;
    color: #333;
}
.totalPages span, .totalSize span {
    color: #0073A9;
    margin: 0 5px;
}
.totalPages {
    margin: 0 10px;
}
#prePage, #nextPage {
    width: 66px;
}
.page_div .current {
    background-color: #0073A9;
    border-color: #0073A9;
    color: #FFFFFF;
    margin-left: 9px;
}
.page_div a {
    min-width: 31px;
    height: 28px;
    border: 1px solid #dce0e0!important;
    text-align: center;
    margin-left: 4px;
    cursor: pointer;
    line-height: 28px;
    color: #666666;
    font-size: 13px;
    display: inline-block;
}

.wrapFixed {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: #000;
    opacity: 0.5;
    z-index: 800;
    display: none;
    /*filter: progid:DXImageTransform.Microsoft.Alpha(opacity=100);兼容IE8透明度*/
}
