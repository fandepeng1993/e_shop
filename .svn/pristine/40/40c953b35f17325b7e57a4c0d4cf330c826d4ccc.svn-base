body{margin:0px;padding:0px; font-size:12px;border: 1px solid #DCDCDC;height: 814px;background:white;}

#top{width:100%;height:90px;float:left;background:#fff ;position:relative;border-bottom: 1px solid #DCDCDC;}

#top .small{width:634px;height:56px;position:absolute;top:16px;left:50%;display:inline;margin:0 0 0 -317px;}

#top .small ul{margin:0;padding:0px;}

#top .small li{width:67px;height:50px;float:left;border:1px solid #000;display:inline;margin:3px 0 0 3px;padding:0px;filter:alpha(opacity=40);opacity:0.4;cursor:pointer;}

#top .small li.hove{filter:alpha(opacity=100);opacity:1;}

#box{width:100%;height:456px;float:left;background:#F5F5F5;overflow:hidden;position:relative;}

#box .bg{width:100%;height:333px;background:#f5f5f5;filter:alpha(opacity=70); opacity:0.7; position:absolute; top:66px; left:0px;}

#list{width:855px;height:446px;position:absolute;top:10px;left:50%;display:inline;margin-left: -428px;}

#list ul {margin:0px;padding:0px; float:left;}

#list li{width:100px;border:3px solid #000;position:absolute;list-style-type:none;overflow:hidden;cursor:pointer;}

#list li img{width:100%;height:100%;vertical-align:top;}

#list li div{width:100%;height:100%;background:#000;filter:alpha(opacity=70); opacity:0.7; position:absolute; z-index:1; top:0px; left:0px;}

#list li .b_tit{width:100%;height:120px;position:absolute; z-index:1; bottom:-100%; left:0px;margin:0px;padding:0px;}

#list li .opacity{width:100%;height:120px;background:#000;filter:alpha(opacity=80); opacity:0.8; position:absolute; z-index:1; bottom:0px; left:0px;}

#list li .tit{width:324px;height:120px;padding:0 10px;position:absolute; z-index:2; bottom:0px; left:0px;}

#list li .tit span{width:100%;height:30px;font-weight:normal;float:left;text-align:left;line-height:30px;color:#fff;border-bottom:1px solid #2f2f2c;margin:0px;padding:0px;}

#list li .tit span em{color:#ba0707;font-style:normal;float:none;}

#list li .tit em{color:#fff;line-height:25px;float:left;margin:0px;padding:5px 0;font-style:normal;}

#top a{width:37px;height:49px;background:url(../../../material/CompanyDetails/companyqualification/images/btn.png) no-repeat;text-decoration:none;position:absolute;z-index:20;top:3px;border-radius: 5px;}

#top a.prev{background-position: -2px -16px;left: -46px;}

#top a.next{background-position: -39px -14px;right: -46px;}

