.companyfeature ul.out-ul-company>li{
    margin-bottom: 20px;
}
.public-title{
    background: #FFFDEF;
    padding: 15px 0;
    border-bottom: 1px solid #D9D9D9;
}
.public-title h4{
    margin: 0;
    text-align: center;
    font-size: 17px;
    position: relative;
}
.public-title h4:after{
    content: '';
    width: 50px;
    height: 1px;
    background: #DCDCDC;
    position: absolute;
    top: 15px;
    left: 310px;
}
.public-title h4:before{
    content: '';
    width: 50px;
    height: 1px;
    background: #DCDCDC;
    position: absolute;
    top: 15px;
    right: 310px;
}
.public-title h4 i{
    color:#47A8F9 ;
    margin-right: 10px;
    font-size: 25px;
}
.enterprise-profile-content{
    background: #fff;
    padding: 20px 30px;
    overflow: hidden;

}
.img-left-wrap img{
    width: 250px;
}
.img-left-wrap{
    float: left;
    width: 36%;
}
.img-right-wrap{
    float: left;
    width: 64%;
}
.img-right-wrap p{
    text-indent:2em;
    word-break: break-all;
    line-height: 25px;
    display: -webkit-box;
    -webkit-line-clamp:6;
    -ms-line-clamp:6;
    -webkit-box-orient: vertical;
    -ms-box-orient: vertical;
     margin-bottom: 0px;
}
.img-right-wrap a{
    color: #FFA200;
    text-decoration: none;
    float: right;
    cursor: pointer;
}
.text-content-info{
    height: 150px;
    overflow: hidden;
    text-overflow: ellipsis;
}

.field-certification-content{
    background: #fff;
    padding: 20px 30px;
    overflow: hidden;
}
.field-left-text ul li i{
    font-style: normal;
    display: inline-block;
    width: 14%;
}
.field-left-text ul li span{
    display: inline-block;
    color: #333;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    width: 82%;
    vertical-align: middle;
}
.field-left-text ul li{
    list-style-type:square;
    list-style-position:inside;
    color: #666666;
    margin-bottom: 10px;
}
.field-left-text{
    float: left;
    width: 63%;
    margin-top: 16px;
}
.field-right-img{
    float: left;
    width: 37%;
    background: #e5e5e5;
    height: 150px;
}

*{margin:0;padding:0;list-style-type:none;}
a,img{border:0;}
/* flexslider */
.flexslider{position:relative;width:100%;height:150px;zoom:1;padding: 20px 30px;}
.flex-viewport{max-height:2000px;-webkit-transition:all 1s ease;-moz-transition:all 1s ease;transition:all 1s ease;}
.flexslider .slides{zoom:1;}
.flex-direction-nav a{width:25px;height:30px;line-height:99em;overflow:hidden;margin:-60px 0 0;display:block;background:url(../../../material/home/ad_ctr.png) no-repeat;position:absolute;top: 123px;z-index:10;cursor:pointer;opacity:1;background-size:30px;}
.flex-direction-nav .flex-next{background-position:-4px -51px;right:3px;}
.flex-direction-nav .flex-prev{left:3px;background-position:-1px -6px;}
/*.flexslider:hover .flex-next{opacity:0.8;filter:alpha(opacity=25);}
.flexslider:hover .flex-prev{opacity:0.8;filter:alpha(opacity=25);}
.flexslider:hover .flex-next:hover, .flexslider:hover .flex-prev:hover{opacity:1;filter:alpha(opacity=50);}*/
.flex-control-nav{width:100%;position:absolute;bottom:10px;text-align:center;display:none;}
.flex-control-nav li{margin:0 5px;display:inline-block;zoom:1;*display:inline;}
.flex-control-paging li a{background:url(../../../material/home/dot.png) no-repeat 0 -16px;display:block;height:16px;overflow:hidden;text-indent:-99em;width:16px;cursor:pointer;}
.flex-control-paging li a.flex-active{background-position:0 0;}


.business-content{
    background: #fff;

}
.business-content ul{
    overflow: hidden;
    border-right: 1px solid #D9D9D9;
}
.business-content ul li{
    float: left;
    overflow: hidden;
    width: 50%;
    border-bottom: 1px solid #D9D9D9;

}
.business-content ul li.li-lastli{
    width: 100%;
}
.business-content ul li i{
    font-style: normal;
    float: left;
    background: #EBEBEB;
    line-height: 50px;
    width: 23%;
    text-align: center;
    border-left: 1px solid #D9D9D9;
    border-right: 1px solid #D9D9D9;
}
.business-content ul li.li-lastli i{
    width: 11.5%;
}
.business-content ul li span{
    float: left;
    line-height: 50px;
    width: 77%;
    padding-left: 20px;
}
.Enterprise-features-content{
    padding: 30px 0;
    background: #fff;

}
.Enterprise-features-content ul{
    overflow: hidden;
}
.Enterprise-features-content ul li{
    float: left;
    width: 24%;
    margin: 0.5%;
}
.Enterprise-features-content ul li img{
    width: 100%;

}