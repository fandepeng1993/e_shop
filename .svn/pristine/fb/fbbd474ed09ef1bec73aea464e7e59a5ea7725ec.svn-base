.ImmediatelyGrabSingle {
	width: 63%;
	margin-left: 18.5%;
}

.ImmediatelyGrabSingle .top {
	width: 100%;
	line-height: 50px;
	min-width: 1200px;
	height: 50px;
}

.ImmediatelyGrabSingle .top .left {
	color: #47A8F9;
	float: left;
}

.ImmediatelyGrabSingle .topContent {
	width: 100%;
	min-width: 1100px;
}

.ImmediatelyGrabSingle .topContent .topContent-title {
	width: 100%;
	height:50px;
	background: #2B8EE3;
	line-height: 50px;
	padding-left: 27px;
}

.ImmediatelyGrabSingle .topContent .topContent-title .left {
	color: #FFF;
	font-size: 18px;
}

.ImmediatelyGrabSingle .topContent .topContent-title .right {
	float: right;
	color: #fff;
	font-size: 16px;
	padding-right: 27px;
}

.ImmediatelyGrabSingle .topContent .topContent-title .right i {
	font-size: 25px;
	float: left;
	margin-right: 5px;
	margin-top: -1px;
}

.ImmediatelyGrabSingle .topContent .topContent-content {
	border: 1px solid #D2D2D2;
}

.ImmediatelyGrabSingle .topContent .topContent-content .table {
	margin-bottom: 0px;
	font-size: 16px;
}

.ImmediatelyGrabSingle .topContent .topContent-content .table td {
	height: 40px;
	line-height: 40px;
	background: #fff;
}

.ImmediatelyGrabSingle .topContent .topContent-content .table td .btn{
	color: #fff;
	background: #FD7322;
	outline: none;
}

.ImmediatelyGrabSingle .requires {
	width: 100%;
	min-width: 1100px;
	border: 1px solid #D2D2D2;
	margin-top: 13px;
	background: #fff;
}

.ImmediatelyGrabSingle .requires .requires-title {
	width: 100%;
	height: 60px;
}

.ImmediatelyGrabSingle .requires .requires-title i {
	float: left;
	margin-top: 5px;
	margin-left: 28px;
	font-size: 35px;
	color: #47A8F9;
}

.ImmediatelyGrabSingle .requires .requires-title .requires-title-p {
	float: left;
	margin-left: 13px;
	font-size: 17px;
	color: #333;
	margin-top: 10px;
}

.ImmediatelyGrabSingle .requires .requires-title .requires-title-p span {
	font-size: 13px;
	display: block;
	margin-top: -3px;
}

.ImmediatelyGrabSingle .requires .requires-content {
	width: 100%;
}

.ImmediatelyGrabSingle .requires .requires-content tr td {
	height: 50px;
	line-height: 50px;
	font-size: 16px;
}

.ImmediatelyGrabSingle .requires .requires-content tr.two td {
	border: 0px;
	line-height: 3px;
}

.ImmediatelyGrabSingle .requires .requires-content tr.two td .dot {
	margin-top: 0px;
}

.ImmediatelyGrabSingle .requires .requires-content tr td.first {
	padding-left: 37px;
}

.ImmediatelyGrabSingle .requires .requires-content tr td .dot {
	width: 5px;
	height: 5px;
	border-radius: 50%;
	background: #7B7B7B;
	display: block;
	float: left;
	margin-top: 23px;
	margin-right: 14px;
}

.ImmediatelyGrabSingle .requires .requires-content .requires-content-bottom table{
	margin-bottom: 0px;
}

.ImmediatelyGrabSingle .requires .requires-content .explain{
	width: 100%;
	height: 80px;
	line-height: 80px;
	font-size: 16px;
	border-top: 1px solid #BFBFBF;
	overflow: hidden;
}

.ImmediatelyGrabSingle .requires .requires-content .explain .left{
	padding-left: 49px;
	width: 16%;
	border-right: 1px solid #BFBFBF;
	float: left;
}
.ImmediatelyGrabSingle .requires .requires-content .explain .right{
	/*padding-left: 49px;
	width: 173px;
	border-right: 1px solid #BFBFBF;*/
	float: right;
	width: 84%;
	height: 100%;
	line-height: normal;
}
#instruction {
	width: 100%;
	height: 100%;
	resize: none;
	border: none;
	outline: none;
	padding-left: 20px;
	padding-right: 20px;
	text-indent: 20px;
	/*overflow: hidden;*/
}

/*笔记本适配*/
.smallScreen .ImmediatelyGrabSingle{
	width: 80%;
	margin-left: 10%;
}
