.FindSpecialOffer {
	/*width: 63%;
	margin-left: 18.5%;*/
	height: 1000px;
}

.FindSpecialOffer .top {
	width: 100%;
	line-height: 36px;
	min-width: 1200px;
	height: 36px;
}

.FindSpecialOffer .top .left {
	color: #47A8F9;
	float: left;
}

.FindSpecialOffer .title {
	width: 100%;
	min-width: 1200px;
	height: 70px;
	line-height: 70px;
}

.FindSpecialOffer .title-top {
	width: 100%;
	height: 70px;
	line-height: 70px;
	background: #fff;
	float: left;
	border-bottom: 1px solid #D9D9D9;
	margin-left: 0px;
}

.FindSpecialOffer .title-top .left {
	float: left;
}

.FindSpecialOffer .title-top .left i {
	font-size: 45px;
	color: #47A8F9;
	padding-left: 20px;
	padding-right: 5px;
}

.FindSpecialOffer .title-top .left .first {
	display: inline-block;
	line-height: 20px;
	font-size: 17px;
	font-weight: bold;
}

.FindSpecialOffer .title-top .left .first .small {
	color: #333;
	font-size: 13px;
}

.FindSpecialOffer .title-top .right {
	float: right;
	padding-right: 10px;
	font-size: 16px;
}

.FindSpecialOffer .title-top .right .yellow {
	color: #FF6600;
	padding-left: 5px;
	padding-right: 5px;
}

.FindSpecialOffer .title-content {
	width: 100%;
	height: 40px;
	line-height: 40px;
	float: left;
	background: #fff;
}

.FindSpecialOffer .title-content .left {
	width: 100px;
	float: left;
	height: 40px;
	line-height: 40px;
	font-size: 15px;
	text-align: center;
}

.FindSpecialOffer .title-content .right {
	float: left;
}

.FindSpecialOffer .title-content .right .right-ul li {
	float: left;
	font-size: 15px;
	padding-left: 20px;
	padding-right: 20px;
	cursor: pointer;
}

.FindSpecialOffer .title-content .right .right-ul li.active {
	color: #47A8F9;
}

/*内容*/
.FindSpecialOffer .content {
	/*float: left;
	width: 1220px;
	margin-left: -20px;*/
}

.FindSpecialOffer .content .card {
	float: left;
	width: 30%;
	min-width: 590px;
	padding: 30px;
	background: #fff;
	margin-top: 20px;
	margin-left: 20px;
}

.FindSpecialOffer .content .card .card-top {
	float: left;
	width: 100%;
}

.FindSpecialOffer .content .card .card-top .left {
	float: left;
}

.FindSpecialOffer .content .card .card-top .right {
	float: left;
	min-width: 357px;
	margin-top: -5px;
}

.FindSpecialOffer .content .card .bottom {
	float: left;
}

.FindSpecialOffer .content .card .card-top .right .one {
	font-size: 22px;
	color: #333;
	margin-top: -12px;
}

.FindSpecialOffer .content .card .card-top .right .two {
	font-size: 14px;
	color: #666;
	border-bottom: 1px solid #D9D9D9;
	padding-bottom: 5px;
}

.FindSpecialOffer .content .card .card-top .right .three, .FindSpecialOffer .content .card .card-top .right .four {
	font-size: 16px;
	color: #333;
}

.FindSpecialOffer .content .card .card-top .right .three .three-right {
	display: block;
	width: 170px;
	float: right;
	text-align: left;
	margin-top: 2px;
}

.FindSpecialOffer .content .card .card-top .right .four .four-right {
	display: block;
	width: 170px;
	float: right;
	text-align: left;
	margin-top: 2px;
}

.FindSpecialOffer .content .card .card-top .right .five {
	font-size: 16px;
	color: #FF0000;
	margin-top: -3px;
}

.FindSpecialOffer .content .card .card-top .right .five .big {
	font-size: 26px;
}

.FindSpecialOffer .content .card .card-top .right .five .price {
	color: #999;
	font-size: 14px;
	text-decoration: line-through;
	float: right;
	margin-top: -25px;
	margin-right: 68px;
}

.FindSpecialOffer .content .card .card-top .right p {
	margin-left: 20px;
	margin-bottom: 3px;
}

.FindSpecialOffer .content .card .bottom .btn {
	width: 197px;
	background: #FF6600;
	height: 40px;
	color: #fff;
	outline: none;
	border-radius: 20px;
	margin-left: 180px;
	margin-top: 10px;
}

/*小屏*/
.smallScreen .FindSpecialOffer{
	/*margin-left: 10%;*/
}

.smallScreen .FindSpecialOffer .content .card .card-top .right {
	float: right;
	width: 337px;
	min-width: 337px;
	margin-top: 0px;
}

.smallScreen .FindSpecialOffer .content .card {
	float: left;
	width: 30%;
	min-width: 530px;
	padding: 10px;
	background: #fff;
	margin-top: 20px;
	margin-left: 20px;
	padding-top: 20px;
	padding-bottom: 20px;
}

.smallScreen .FindSpecialOffer .title{
	min-width: 1080px;
}

.smallScreen .FindSpecialOffer .content .card .card-top .right .five .price {
	margin-right: 40px;
}
