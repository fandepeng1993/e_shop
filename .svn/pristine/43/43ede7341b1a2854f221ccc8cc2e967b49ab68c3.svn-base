.memberCenter {
	width: 100%;
	height: 680px;
	min-width: 1100px;
}

.memberCenter .mem-title {
	width: 100%;
	height: 78px;
	color: #fff;
	min-width: 1100px;
	background: #2C84E7;
}

.memberCenter .mem-title-in {
	width: 63%;
	margin-left: 18.5%;
	min-width: 1100px;
}

.memberCenter .mem-title ul {
	width: 520px;
	float: left;
}

.memberCenter .mem-title ul li {
	display: inline-block;
	font-size: 16px;
	width: 119px;
	height: 73px;
	margin-top: 2px;
	text-align: center;
}

.memberCenter .mem-title ul a{
	color: #fff;
}

.memberCenter .mem-title ul li p {
	margin-top: -12px;
}

.memberCenter .mem-title ul li.active {
	background: #fff;
	color: #2C84E7;
}

.memberCenter .mem-title ul li.active a{
	background: #fff;
	color: #2C84E7;
}

.memberCenter .mem-title ul li .icon-huiyuanzhongxin {
	font-size: 40px;
}

.memberCenter .mem-title ul li .icon-dianpu {
	font-size: 25px;
	line-height: 50px;
}

.memberCenter .mem-title ul li .icon-shouye1 {
	font-size: 28px;
	line-height: 45px;
}

.memberCenter .mem-title ul li .icon-zhuxiaodenglu {
	font-size: 28px;
	line-height: 50px;
}

.memberCenter .mem-title .mem-title-right {
	display: inline-block;
	font-size: 16px;
}

.memberCenter .mem-title .mem-title-right {
	float: right;
	margin-top: 13px;
	margin-right: 30px;
}

.memberCenter .mem-title .mem-title-right img {
	float: left;
	margin-right: 10px;
}

.memberCenter .mem-title .mem-title-right span.small {
	font-size: 12px;
}

.memberCenter .mem-title .mem-title-right span.small i {
	margin-left: 5px;
	color: #FFE6AA;
}

.memberCenter .mem-content {
	width: 63%;
	margin-left: 18.5%;
	float: left;
	min-width: 1100px;
	margin-top: 0px;
}

.memberCenter .mem-content .mem-left {
	width: 262px;
	height: 545px;
	float: left;
}

.memberCenter .mem-left ul {
	width: 100%;
	height: 100%;
	border: 1px solid #B5B5B5;
	background: #fff;
}

.memberCenter .mem-left ul li {
	width: 100%;
	height: 68px;
	font-size: 18px;
	color: #333;
	padding-top: 22px;
	border-bottom: 1px solid #B5B5B5;
}

.memberCenter .mem-left ul li a {
	color: #333;
}

.memberCenter .mem-left ul li .first {
	padding-left: 25px;
	font-size: 20px;
	margin-right: 10px;
	float: left;
}

.memberCenter .mem-left ul li .icon-jiantou1 {
	float: right;
	margin-right: 20px;
}

.memberCenter .mem-left ul li .icon-shouhuodizhi {
	padding-left: 0px;
	margin-right: -10px;
}

.memberCenter .mem-left ul li .icon-wodexiaoxi {
	font-size: 18px;
	padding-left: 23px;
}

.memberCenter .mem-left ul li .icon-xiugaimima {
	padding-left: 22px;
}

.memberCenter .mem-left ul li .icon-shoucang {
	padding-left: 22px;
}

.memberCenter .mem-left ul li .icon-wuliu1 {
	padding-left: 22px;
}

.memberCenter .mem-left ul li.active ,.memberCenter .mem-left ul li.active a{
	background: #3D91F0;
	color: #fff;
}

.memberCenter .mem-right {
	width: 900px;
	height: 477px;
	margin-left: 10px;
	float: left;
	background: #fff;
	border: 1px solid #BCBCBC;
	position: relative;
}

.memberCenter .mem-right .mem-right-ul{
	width: 100%;
	height: 53px;
	background: #eee;
	border-bottom: 1px solid #BCBCBC;
}

.memberCenter .mem-right .mem-right-ul li{
	width: 151px;
	height: 53px;
	line-height: 53px;
	font-size: 18px;
	color: #333;
	text-align: center;
	display: inline-block;
	border-right: 1px solid #BCBCBC;
	position: relative;
}

.memberCenter .mem-right .mem-right-ul li .sanjiao{
	position: absolute;
	width: 0;
	height: 0;
	border-left: 4px solid transparent;
	border-right: 4px solid transparent;
	border-bottom: 5px solid #FF6600;
	left: 50%;
	margin-left: -2px;
	bottom: 0px;
	display: none;
}

.memberCenter .mem-right .mem-right-ul li.active{
	background: #fff;
	border-bottom: 2px solid #FF6600;
}

.memberCenter .mem-right .mem-right-ul li.active .sanjiao{
	display: block;
}

.memberCenter .mem-right .mem-right-l {
	padding: 20px 30px 0px 30px;
	font-size: 16px;
	width: 56%;
	float: left;
}

.memberCenter .mem-right .mem-right-l p {
	margin-bottom: 20px;
}

.memberCenter .mem-right .mem-right-l p.space {
	letter-spacing: 7px;
}

.memberCenter .mem-right .mem-right-l p.sma {
	letter-spacing: 3px;
}

.memberCenter .mem-right .mem-right-l p input[type="text"] {
	padding-left: 5px;
	padding-right: 5px;
	border-radius: 4px;
	border: 1px solid #BEBEBE;
	height: 30px;
}

.memberCenter .mem-right .mem-right-l p .icon-anquanshezhi-yanzhengtongguo{
	color: #3D91F0;
	font-size: 20px;
}

.memberCenter .mem-right .mem-right-l p span {
	color: #3D91F0;
	padding-left: 5px;
	padding-right: 5px;
}

.memberCenter .mem-right .mem-right-l p .update-btn {
	color: #FF6600;
	background: none;
	outline: none;
}

.memberCenter .mem-right .mem-right-l p .confirm {
	color: #fff;
	background: #2C84E7;
	outline: none;
	padding-left: 20px;
	padding-right: 20px;
}

.memberCenter .mem-right .mem-right-r {
	float: left;
	width: 44%;
}

.memberCenter .mem-right .mem-right-r .mem-right-box  {
	width: 80%;
	margin-left: 5%;
	border: 1px dashed #FF6600;
	margin-top: 5%;
}

.memberCenter .mem-right .mem-right-r .mem-right-box .img-p {
	text-align: center;
	margin-bottom: 20px;
}

.memberCenter .mem-right .mem-right-r img {
	font-size: 13px;
	margin: 30px 0  20px 90px;
}

.memberCenter .mem-right .mem-right-r .img-p {
	font-size: 13px;
}

.line-po{
	position: absolute;
	height: 150px;
	
	background: #ccc;
	width: 1px;
	left: 50%;
	top: 90px;
}

/*中屏适配*/
.middleScreen .memberCenter .mem-title-in {
	width: 69%;
	margin-left: 15.5%;
}

.middleScreen .memberCenter .mem-content {
	width: 69%;
	margin-left: 15.5%;
	float: left;
}

.middleScreen .memberCenter .mem-right {
	width: 815px;
}

.middleScreen .memberCenter .mem-right .mem-right-bt  input[type="text"] {
	width: 165px;
}
/*小屏适配*/
.smallScreen .memberCenter .mem-title-in {
	width: 80%;
	margin-left: 10%;
}

.smallScreen .memberCenter .mem-content {
	width: 80%;
	margin-left: 10%;
	float: left;
}

.smallScreen .memberCenter .mem-right {
	width: 815px;
}

.smallScreen .memberCenter .mem-right .mem-right-bt  input[type="text"] {
	width: 165px;
}
